{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
  ],
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": 1,
    "no-underscore-dangle": 1,
    "global-require": 1,
    "func-names": 1,
    "no-unneeded-ternary": 1,
    "no-param-reassign": 1,
  }
}
